{
	"schema_version": 1,
	"quilt_loader":
	{
		"group": "virtuoel",
		"id": "gift_wrap",
		"version": "${version}",
		"metadata":
		{
			"name": "Gift Wrap",
			"description": "A loader plugin test.",
			"contributors":
			{
				"Virtuoel": "Author"
			}
		},
		"intermediate_mappings": "net.fabricmc:intermediary",
		"language_adapters":
		{
			"gift_wrap": "virtuoel.gift_wrap.GiftWrapLanguageAdapter"
		}
	},
	"mixin":
	[
		"gift_wrap.mixins.json"
	],
	"modmenu":
	{
		"badges":
		[
			"library"
		]
	},
	"experimental_quilt_loader_plugin":
	{
		"class": "virtuoel.gift_wrap.GiftWrapPlugin",
		"packages":
		[
			"virtuoel.gift_wrap.shadow.nightconfig.core",
			"virtuoel.gift_wrap.shadow.nightconfig.core.conversion",
			"virtuoel.gift_wrap.shadow.nightconfig.core.file",
			"virtuoel.gift_wrap.shadow.nightconfig.core.io",
			"virtuoel.gift_wrap.shadow.nightconfig.core.utils",
			"virtuoel.gift_wrap.shadow.nightconfig.toml",
			"virtuoel.gift_wrap.shadow.mappingio",
			"virtuoel.gift_wrap.shadow.mappingio.adapter",
			"virtuoel.gift_wrap.shadow.mappingio.format",
			"virtuoel.gift_wrap.shadow.mappingio.tree",
			"virtuoel.gift_wrap"
		]
	}
}
